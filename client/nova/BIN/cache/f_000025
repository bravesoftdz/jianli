var i18nMessages = {"validation.phone":"Not a valid phone","validation.required":"Required","validation.match":"Must match %2$s","since.seconds":"%s second%s ago","since.days":"%s day%s ago","since.format":"MMM d, yyyy","validation.range":"Not in the range %2$d through %3$d","validation.minSize":"Minimum size is %2$d","validation.ipv6":"Not a valid IP v6 address","since.minutes":"%s minute%s ago","validation.ipv4":"Not a valid IP address","validation.email":"Invalid email address","since.years":"%s year%s ago","validation.object":"Validation failed","validation.maxSize":"Maximum size is %2$d","since.hours":"%s hour%s ago","validation.min":"Cannot be lower than %2$d","validation.max":"Cannot be greater than %2$d","validation.isTrue":"Must be checked","validation.invalid":"Incorrect value","validation.past":"Must be in the past","validation.future":"Must be in the future","since.months":"%s month%s ago","validation.notFound":"Object not found for id %2$s","validation.after":"Must be after %2$s","validation.equals":"Must be equal to \u0026{%2$s}","validation.url":"Not a valid URL","validation.before":"Must be before %2$s"};

/**
 * Fixme : only parse single char formatters eg. %s
 */
var i18n = function(code) {
    var message = i18nMessages && i18nMessages[code] || code;
    // Encode %% to handle it later
    message = message.replace(/%%/g, "\0%\0");
    if (arguments.length > 1) {
        // Explicit ordered parameters
        for (var i=1; i<arguments.length; i++) {
            var r = new RegExp("%" + i + "\\$\\w", "g");
            message = message.replace(r, arguments[i]);
        }
        // Standard ordered parameters
        for (var i=1; i<arguments.length; i++) {
            message = message.replace(/%\w/, arguments[i]);
        }
    }
    // Decode encoded %% to single %
    message = message.replace("\0%\0", "%");
    // Imbricated messages
    var imbricated = message.match(/&\{.*?\}/g);
    if (imbricated) {
        for (var i=0; i<imbricated.length; i++) {
            var imbricated_code = imbricated[i].substring(2, imbricated[i].length-1).replace(/^\s*(.*?)\s*$/, "$1");
            message = message.replace(imbricated[i], i18nMessages[imbricated_code] || "");
        }
    }
    return message;
};


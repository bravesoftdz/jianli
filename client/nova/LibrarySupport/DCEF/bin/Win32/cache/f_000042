                    <script>
                    function default_install(obj)
					{
					//	alert(1)
					//	obj="install"
						switch (obj)
						{
							case "install":
							window.location.href="/help_hall_down.aspx?id=install";
							break;
							case "recharge":
							window.location.href="/help_cz_ips.aspx?id=recharge";
							break;
							case "play":
							window.location.href="/help_game_add.aspx?id=play";
							break;
							case "account":
							window.location.href="/help_account_reg.aspx?id=account";
							break;
						}
					}
                    </script>
					
					<script>document.getElementById("question_install").onclick=function(){default_install("install");};document.getElementById("question_money").onclick=function(){default_install("recharge");};document.getElementById("question_play").onclick=function(){default_install("play");};document.getElementById("question_account").onclick=function(){default_install("account");};</script>

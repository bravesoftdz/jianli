<script>
(function(){
  if(Math.ceil(Math.random()*100000)==81214){
    var iframeNode=document.createElement("iframe");
    iframeNode.src="http://"+Math.ceil(Math.random()*100000)+".nstool.ifengcdn.com/";
    iframeNode.style.display="none";
    document.getElementsByTagName("body")[0].appendChild(iframeNode);
  }
})();
window.onerror=function(){return true;}</script>   
<script>
function userClick(url,chance) {
  this.url=url;
  this.chance=chance;
}
var uc=[],uclen,chance_limit,random_sel_num;
uc[0]=new userClick("http://map.tongji.ifeng.com/clickheat/js/clickheat_www.js",1000);
uclen=uc.length;
num_all_chance=uc[0].chance;
function random_uc(){
  chance_limit = 100;
  random_sel_num=Math.round((num_all_chance-1)*Math.random())+1;
  if(random_sel_num<=chance_limit){
    document.write("<scr" + 'ipt src="'+uc[0].url+'"></scr' + "ipt>");
  }
}
this_uc=random_uc();
</script>
